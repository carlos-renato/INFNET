algoritmo "Conversao"
// Função :
// Autor :
// Data : 18/09/2014
// Seção de Declarações 
var
   reais, dolares, cambio : real
inicio
// Seção de Comandos
   escreva ("Quantidade de reais que tenho: ")
   leia(reais)
   escreva ("Cotação do dólar: ")
   leia (cambio)
   dolares <- reais/cambio
   escreva ("Posso ter US$",dolares)
fimalgoritmo
