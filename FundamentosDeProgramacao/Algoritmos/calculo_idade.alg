algoritmo "Creusa01"
// Função :
// Autor :
// Data : 18/09/2014
// Seção de Declarações 
var
   anoAtual, anoNasc, idade  : inteiro
inicio
// Seção de Comandos
   escreva ("Em que ano nós estamos? ")
   leia (anoAtual)
   escreva ("Em que ano eu nasci? ")
   leia (anoNasc)
   idade <- anoAtual-anoNasc
   escreva ("Minha idade é ", idade)
   
fimalgoritmo
