algoritmo "semnome"
// Função :
// Autor :
// Data : 18/09/2014
// Seção de Declarações 
var
   cont, n, salto : inteiro
inicio
// Seção de Comandos
   escreva ("Deseja contar até quanto? ")
   leia (n)
   escreva ("De quanto em quanto? ")
   leia (salto)
   cont <- 0
   enquanto cont <= n faca
            escreval (cont)
            cont <- cont + salto
   fimenquanto


fimalgoritmo
