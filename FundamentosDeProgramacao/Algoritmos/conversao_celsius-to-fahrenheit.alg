algoritmo "semnome"
// Função :
// Autor :
// Data : 18/09/2014
// Seção de Declarações 
var
f, c : real

inicio
// Seção de Comandos
   escreval ("Conversão de Fahrenheit para Celsius.")
   escreva ("Qual é a temperatura em Fahrenheit? ")
   leia (f)
   c <- (f - 32) / 1.8
   escreva ("Isso equivale a ", c:2, " graus Celsius.")
fimalgoritmo
