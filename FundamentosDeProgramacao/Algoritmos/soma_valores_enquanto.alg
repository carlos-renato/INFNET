algoritmo "semnome"
// Função :
// Autor :
// Data : 18/09/2014
// Seção de Declarações 
var
   cont, n, s : inteiro
inicio
// Seção de Comandos
   cont <- 0
   s <- 0

   Enquanto cont < 10 faca
            escreva ("Digite o número: ")
            leia (n)
            s <- s + n
            cont <- cont + 1
   FimEnquanto
   escreva (s)
fimalgoritmo
